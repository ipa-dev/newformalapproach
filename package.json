{
  "name": "newformalapproach",
  "version": "1.0.0",
  "description": "New Website for Formal Approach",
  "main": "index.js",
  "scripts": {
    "build-task:scss-compile": "node-sass-chokidar --source-map true client/scss/ -o client/css",
    "build-task:autoprefixer": "postcss client/css/*.css --use autoprefixer -d client/css",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'client/scss/**/*.scss' -c 'npm run sass:build'",
    "webpack:build": "webpack --config webpack.config.js",
    "start": "node index.js",
    "compile": "npm-run-all -p sass:* webpack:*",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipa-dev/newformalapproach.git"
  },
  "author": "Mason Watson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ipa-dev/newformalapproach/issues"
  },
  "homepage": "https://github.com/ipa-dev/newformalapproach#readme",
  "dependencies": {
    "ajv": "^6.5.3",
    "autoprefixer": "^9.1.2",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "chokidar-cli": "^1.2.0",
    "deep-freeze-node": "^1.1.3",
    "enzyme": "^3.4.4",
    "expect": "^1.20.2",
    "expect-enzyme": "^1.3.0",
    "jquery": "^3.3.1",
    "keystone": "^4.0.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "popper.js": "^1.14.4",
    "postcss-cli": "^6.0.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  }
}
